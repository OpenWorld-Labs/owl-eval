# API URL for backend
NEXT_PUBLIC_API_URL=http://localhost:5000

# Base URL for the frontend (used for Prolific callbacks)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Stack Auth configuration (OPTIONAL - leave empty for dev mode without authentication)
# Get these from https://app.stack-auth.com after creating a project
NEXT_PUBLIC_STACK_PROJECT_ID=
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=
STACK_SECRET_SERVER_KEY=

# Database configuration for Neon PostgreSQL
# For development, you can use a local PostgreSQL database
DATABASE_URL="postgresql://username:password@localhost:5432/evaluation?sslmode=disable"