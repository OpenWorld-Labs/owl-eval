# OWL Evaluation Framework - Environment Configuration
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/owl_eval"

# Authentication (Stack Auth)
NEXT_PUBLIC_STACK_PROJECT_ID="your-stack-project-id"
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY="your-stack-client-key"
STACK_SECRET_SERVER_KEY="your-stack-server-secret"

# Application Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"

# Prolific Integration (Optional)
PROLIFIC_API_TOKEN="your-prolific-api-token"

# Storage Configuration
# AWS S3 (Optional)
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your-s3-bucket"

# Local Storage
DATA_DIR="./data"
VIDEO_UPLOAD_DIR="./uploads/videos"

# Evaluation Settings
TARGET_EVALUATIONS_PER_COMPARISON=5
MAX_VIDEO_SIZE_MB=100
SUPPORTED_VIDEO_FORMATS="mp4,webm,mov"

# Development Settings
DEBUG_MODE="false"
LOG_LEVEL="info"